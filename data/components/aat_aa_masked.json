{
  "name": "aat_aa_masked",
  "classification": "alignment / pairwise",
  "description": "none",
  "tool_execution": [
    {
      "name": "dps",
      "executable": "$;DPS_EXEC$;"
    },
    {
      "name": "ext",
      "executable": "$;EXT_EXEC$;"
    },
    {
      "name": "ext collapse",
      "executable": "$;EXT_COLLAPSE_EXEC$;"
    },
    {
      "name": "filter",
      "executable": "$;FILTER_EXEC$;"
    },
    {
      "name": "relocate query seq",
      "executable": "cp"
    },
    {
      "name": "nap",
      "executable": "$;NAP_EXEC$;"
    },
    {
      "name": "remove temp query seq",
      "executable": "rm"
    },
    {
      "name": "remove raw dps output",
      "executable": "rm"
    },
    {
      "name": "remove raw ext output",
      "executable": "rm"
    },
    {
      "name": "remove raw ext collapsed output",
      "executable": "rm"
    },
    {
      "name": "remove raw filter output",
      "executable": "rm"
    },
    {
      "name": "ensure btab existence",
      "executable": "touch"
    },
    {
      "name": "relocate btab",
      "executable": "mv"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "nap2bsml",
        "name": "nap2bsml",
        "input": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.nap.btab",
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "btab_output_list",
    "nap_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "DPS_OPTS",
      "default": "-f 100 -i 30 -a 2000"
    },
    {
      "name": "MATRIX",
      "default": "/usr/local/common/BS"
    },
    {
      "name": "EXT_OPTS",
      "default": ""
    },
    {
      "name": "FILTER_OPTS",
      "default": "-c 20"
    },
    {
      "name": "NAP_OPTS",
      "default": ""
    },
    {
      "name": "CUTOFF_IDENTITY",
      "default": ""
    },
    {
      "name": "CUTOFF_SIMILARITY",
      "default": ""
    },
    {
      "name": "COMPRESS_BSML_OUTPUT",
      "default": "1"
    },
    {
      "name": "COMPRESS_BTAB_OUTPUT",
      "default": "1"
    },
    {
      "name": "COMPRESS_NAP_OUTPUT",
      "default": "1"
    }
  ]
}