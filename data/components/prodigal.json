{
  "name": "prodigal",
  "classification": "feature prediction / genes",
  "description": "none",
  "tool_execution": [
    {
      "name": "prodigal",
      "executable": "$;PRODIGAL_EXEC$;"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "prodigal2bsml",
        "name": "prodigal2bsml",
        "input": null,
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "raw_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "OUTPUT_FORMAT",
      "default": "sco"
    },
    {
      "name": "PRODIGAL_OPTS",
      "default": "-c"
    },
    {
      "name": "TRANSLATION_TABLE",
      "default": ""
    },
    {
      "name": "PROJECT_ABBREVIATION",
      "default": ""
    }
  ]
}