{
  "name": "ncbi-blastp",
  "classification": "alignment / pairwise",
  "description": "none",
  "tool_execution": [
    {
      "name": "create output directory",
      "executable": "mkdir"
    },
    {
      "name": "ncbi-blastp",
      "executable": "{{BLASTALL_EXEC}}"
    },
    {
      "name": "dtd validation",
      "executable": "{{BIN_DIR}}/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "blast2btab",
        "name": "convert raw to btab",
        "input": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.raw",
        "output": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.btab"
      },
      {
        "script": "blast2bsml",
        "name": "convert raw to bsml",
        "input": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.raw",
        "output": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "btab_output_list",
    "raw_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "MATRIX",
      "default": "BLOSUM62"
    },
    {
      "name": "EXPECT",
      "default": "1e-5"
    },
    {
      "name": "FILTER",
      "default": "T"
    },
    {
      "name": "DATABASE_MATCHES",
      "default": "150"
    },
    {
      "name": "DESCRIPTIONS",
      "default": "150"
    },
    {
      "name": "OTHER_OPTS",
      "default": ""
    },
    {
      "name": "COMPRESS_RAW_OUTPUT",
      "default": "0"
    },
    {
      "name": "COMPRESS_BSML_OUTPUT",
      "default": "0"
    },
    {
      "name": "FILTER_HSPS_FOR_STATS",
      "default": "1"
    },
    {
      "name": "SPLIT_BSML",
      "default": "0"
    },
    {
      "name": "SPLIT_BTAB",
      "default": "0"
    }
  ]
}