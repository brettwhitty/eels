{
  "name": "hmmls",
  "classification": "alignment / model",
  "description": "none",
  "tool_execution": [
    {
      "name": "replace fasta headers",
      "executable": "$;BIN_DIR$;/replace_fasta_headers.pl"
    },
    {
      "name": "hmmls",
      "executable": "$;HMMLS_EXEC$;"
    },
    {
      "name": "fix output file",
      "executable": "$;BIN_DIR$;/replace_fasta_headers.pl"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "hmmls2bsml",
        "name": "hmmls2bsml",
        "input": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.raw",
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "raw_output_list",
    "htab_output_list",
    "bsml_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "OTHER_OPTS",
      "default": "-c"
    },
    {
      "name": "COMPRESS_BSML_OUTPUT",
      "default": "0"
    },
    {
      "name": "THRESHOLD_CUTOFF",
      "default": "0"
    },
    {
      "name": "SEARCH_WINDOW",
      "default": "1000"
    },
    {
      "name": "MODEL_NAME",
      "default": ""
    },
    {
      "name": "MODEL_MOL_TYPE",
      "default": "aa"
    },
    {
      "name": "MODEL_MOL_CLASS",
      "default": "polypeptide"
    },
    {
      "name": "QUERY_MOL_TYPE",
      "default": "aa"
    },
    {
      "name": "QUERY_MOL_CLASS",
      "default": "polypeptide"
    },
    {
      "name": "LEAVE_FASTA_HEADERS",
      "default": "0"
    }
  ]
}