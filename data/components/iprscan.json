{
  "name": "iprscan",
  "classification": "alignment / model",
  "description": "none",
  "tool_execution": [
    {
      "name": "blastprodom",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "coils",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "hmmpir",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "hmmpfam",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "hmmsmart",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "hmmtigr",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "fprintscan",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "scanregexp",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "profilescan",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "superfamily",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "seg",
      "executable": "{{IPRSCAN_EXEC}}"
    },
    {
      "name": "combine raw output",
      "executable": "find"
    },
    {
      "name": "dtd validation",
      "executable": "{{BIN_DIR}}/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "iprscan2bsml",
        "name": "iprscan2bsml",
        "input": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.raw",
        "output": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "raw_output_list",
    "bsml_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "SEQ_TYPE",
      "default": "p"
    },
    {
      "name": "COMPRESS_BSML_OUTPUT",
      "default": "1"
    }
  ]
}