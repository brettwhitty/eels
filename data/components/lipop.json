{
  "name": "lipop",
  "classification": "functional prediction",
  "description": "none",
  "tool_execution": [
    {
      "name": "lipop",
      "executable": "$;LIPOP_EXEC$;"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "lipop2bsml",
        "name": "lipop2bsml",
        "input": null,
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "raw_output_list",
    "bsml_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "CUTOFF",
      "default": "-3"
    },
    {
      "name": "PROJECT_ABBR",
      "default": "$;PROJECT$;"
    }
  ]
}