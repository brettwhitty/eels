{
  "name": "targetp",
  "classification": "functional prediction",
  "description": "none",
  "tool_execution": [
    {
      "name": "targetp",
      "executable": "{{TARGETP_EXEC}}"
    },
    {
      "name": "dtd validation",
      "executable": "{{BIN_DIR}}/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "targetp2bsml",
        "name": "targetp2bsml",
        "input": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.raw",
        "output": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "raw_output_list",
    "bsml_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "ORGANISM_TYPE",
      "default": "P"
    },
    {
      "name": "CHLOROPLAST_PREDICTION_CUTOFF",
      "default": "0.00"
    },
    {
      "name": "SECRETORY_PATHWAY_PREDICTION_CUTOFF",
      "default": "0.00"
    },
    {
      "name": "MITOCHONDRIAL_PREDICTION_CUTOFF",
      "default": "0.00"
    },
    {
      "name": "OTHER_LOCATION_PREDICTION_CUTOFF",
      "default": "0.00"
    },
    {
      "name": "OTHER_OPTS",
      "default": ""
    }
  ]
}