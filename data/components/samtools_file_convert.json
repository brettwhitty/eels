{
  "name": "samtools_file_convert",
  "classification": "RNA-Seq",
  "description": "converts alignment files between SAM and BAM file formats",
  "tool_execution": [
    {
      "name": "samtools_file_convert",
      "executable": "$;BIN_DIR$;/samtools_file_convert"
    }
  ],
  "converters": {
    "main": [],
    "iterator": []
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "REFERENCE_FASTA",
      "default": ""
    },
    {
      "name": "SAMTOOLS_BIN_DIR",
      "default": "$;SAMTOOLS_BIN$;"
    },
    {
      "name": "SAMTOOLS_VIEW_PARAMETERS",
      "default": ""
    },
    {
      "name": "SAMTOOLS_SORT_PARAMETERS",
      "default": ""
    },
    {
      "name": "OTHER_ARGS",
      "default": "--v"
    }
  ]
}