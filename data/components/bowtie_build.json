{
  "name": "bowtie_build",
  "classification": "RNA-Seq",
  "description": "generates bowtie index files for the given reference fasta file",
  "tool_execution": [
    {
      "name": "bowtie_build",
      "executable": "{{BIN_DIR}}/bowtie_build"
    }
  ],
  "converters": {
    "main": [],
    "iterator": []
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "BOWTIE_BIN_DIR",
      "default": "{{BOWTIE_BUILD_BIN}}"
    },
    {
      "name": "BOWTIE_INDEX_PREFIX",
      "default": "ref"
    },
    {
      "name": "OTHER_PARAMETERS",
      "default": ""
    },
    {
      "name": "OTHER_ARGS",
      "default": "--v"
    }
  ]
}