{
  "name": "phobos",
  "classification": "sequence / masking",
  "description": "none",
  "tool_execution": [
    {
      "name": "phobos",
      "executable": "{{PHOBOS_EXEC}}"
    },
    {
      "name": "dtd validation",
      "executable": "{{BIN_DIR}}/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "phobos2bsml",
        "name": "phobos2bsml",
        "input": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_NAME}}.out",
        "output": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "raw_output_list",
    "fasta_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "SEARCH_MODE",
      "default": "imperfect"
    },
    {
      "name": "INDEL_SCORE",
      "default": "-5"
    },
    {
      "name": "MISMATCH_SCORE",
      "default": "-5"
    },
    {
      "name": "MINSCORE",
      "default": "6"
    },
    {
      "name": "MINLENGTH",
      "default": "0"
    },
    {
      "name": "MINUNITLEN",
      "default": "1"
    },
    {
      "name": "MAXUNITLEN",
      "default": "10"
    },
    {
      "name": "OTHER_OPTS",
      "default": ""
    }
  ]
}