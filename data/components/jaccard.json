{
  "name": "jaccard",
  "classification": "clustering",
  "description": "none",
  "tool_execution": [
    {
      "name": "create output directory",
      "executable": "mkdir"
    },
    {
      "name": "generate clusters",
      "executable": "$;BIN_DIR$;/clusterBsmlPairwiseAlignments"
    },
    {
      "name": "jaccard2fasta",
      "executable": "$;BIN_DIR$;/CogProteinFastaNoMap"
    },
    {
      "name": "create fasta list",
      "executable": "find"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "bsml2idlookup",
        "name": "make asmbl lookup",
        "input": null,
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_GENOME$;.asmbl.lookup"
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "fasta_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "PVALCUT",
      "default": "1e-5"
    },
    {
      "name": "MAX_MULTI_ALIGNMENT",
      "default": "30"
    },
    {
      "name": "PIDENTITY_CUTOFF",
      "default": "80"
    },
    {
      "name": "PSIMILARITY_CUTOFF",
      "default": ""
    },
    {
      "name": "PCOVERAGE_CUTOFF",
      "default": "70"
    },
    {
      "name": "LINKSCORE",
      "default": ".6"
    },
    {
      "name": "USE_FEATURE_IDS_IN_FASTA",
      "default": "0"
    }
  ]
}