{
  "name": "velvet",
  "classification": "assembly",
  "description": "none",
  "tool_execution": [
    {
      "name": "create velveth temp directory",
      "executable": "mkdir"
    },
    {
      "name": "create velvetg temp directory",
      "executable": "mkdir"
    },
    {
      "name": "velveth",
      "executable": "$;VELVETH_EXEC$;"
    },
    {
      "name": "velveth post-processing",
      "executable": "$;BIN_DIR$;/velveth_post_processing"
    },
    {
      "name": "velvetg pre-processing",
      "executable": "$;BIN_DIR$;/velvetg_pre_processing"
    },
    {
      "name": "velvetg",
      "executable": "$;VELVETG_EXEC$;"
    },
    {
      "name": "velvetg post-processing",
      "executable": "$;BIN_DIR$;/velvetg_post_processing"
    }
  ],
  "converters": {
    "main": [],
    "iterator": []
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "sequence_output_list",
    "roadmap_output_list",
    "contig_output_list",
    "stats_output_list",
    "graph_output_list",
    "amos_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "FILE_FORMAT",
      "default": "[-fasta|-fastq|-fasta.gz|-fastq.gz|-eland|-gerald| ]"
    },
    {
      "name": "READ_TYPE",
      "default": "[-short|-shortPaired|-short2|-shortPaired2|-long|-longPaired]"
    },
    {
      "name": "HASH_LENGTH",
      "default": ""
    },
    {
      "name": "VELVETH_OPTS",
      "default": ""
    },
    {
      "name": "VELVETG_OPTS",
      "default": ""
    }
  ]
}