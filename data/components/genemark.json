{
  "name": "genemark",
  "classification": "feature prediction / genes",
  "description": "none",
  "tool_execution": [
    {
      "name": "genemark",
      "executable": "$;GMHMME3_EXEC$;"
    },
    {
      "name": "migrate output file",
      "executable": "mv"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "genemark2bsml",
        "name": "genemark2bsml",
        "input": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.genemark.raw",
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.genemark.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "raw_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "MODEL_FILE",
      "default": ""
    }
  ]
}