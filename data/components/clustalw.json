{
  "name": "clustalw",
  "classification": "alignment / multiple",
  "description": "",
  "tool_execution": [
    {
      "name": "relocate query file",
      "executable": "cp"
    },
    {
      "name": "clustalw",
      "executable": "$;CLUSTALW_EXEC$;"
    },
    {
      "name": "validate clustalw output",
      "executable": "$;BIN_DIR$;/validate_clustalw_output"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "MSF2Bsml",
        "name": "msf2bsml",
        "input": null,
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "raw_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "OUTPUT_FORMAT",
      "default": "gcg"
    },
    {
      "name": "ANALYSIS_CONF",
      "default": "$;REPOSITORY_ROOT$;/workflow/runtime/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;/$;COMPONENT_NAME$;.$;OUTPUT_TOKEN$;.final.config"
    },
    {
      "name": "DESCRIPTION",
      "default": "none"
    },
    {
      "name": "COMPRESS_RAW_OUTPUT",
      "default": "1"
    },
    {
      "name": "TYPE",
      "default": "protein"
    }
  ]
}