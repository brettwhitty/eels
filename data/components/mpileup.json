{
  "name": "mpileup",
  "classification": "alignment",
  "description": "samtools mpileup",
  "tool_execution": [
    {
      "name": "create output directory",
      "executable": "mkdir"
    },
    {
      "name": "create temp directory",
      "executable": "mkdir"
    },
    {
      "name": "mpileup Execution",
      "executable": "$;BIN_DIR$;/mpileup_wrapper"
    }
  ],
  "converters": {
    "main": [],
    "iterator": []
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "mpileup_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "OTHER_MPILEUP_PARAMETERS",
      "default": "-B -C50"
    }
  ]
}