{
  "name": "aat_na",
  "classification": "alignment / pairwise",
  "description": "none",
  "tool_execution": [
    {
      "name": "dds",
      "executable": "$;DDS_EXEC$;"
    },
    {
      "name": "ext",
      "executable": "$;EXT_EXEC$;"
    },
    {
      "name": "ext collapse",
      "executable": "$;EXT_COLLAPSE_EXEC$;"
    },
    {
      "name": "filter",
      "executable": "$;FILTER_EXEC$;"
    },
    {
      "name": "remove raw dds output",
      "executable": "rm"
    },
    {
      "name": "remove raw ext output",
      "executable": "rm"
    },
    {
      "name": "remove raw ext collapsed output",
      "executable": "rm"
    },
    {
      "name": "remove raw filter output",
      "executable": "rm"
    },
    {
      "name": "ensure btab existence",
      "executable": "touch"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "$;GAP2_EXEC$;",
        "name": "gap2",
        "input": null,
        "output": null
      },
      {
        "script": "gap22bsml",
        "name": "gap22bsml",
        "input": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.gap2.btab",
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "btab_output_list",
    "gap2_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "DDS_OPTS",
      "default": "-f 100 -i 30 -o 75 -p 70 -a 2000"
    },
    {
      "name": "DATABASE_PATH",
      "default": ""
    },
    {
      "name": "GAP2_DATABASE_PATH",
      "default": "$;DATABASE_PATH$;"
    },
    {
      "name": "EXT_OPTS",
      "default": ""
    },
    {
      "name": "FILTER_OPTS",
      "default": "-c 10"
    },
    {
      "name": "GAP2_OPTS",
      "default": ""
    },
    {
      "name": "COMPRESS_BSML_OUTPUT",
      "default": "1"
    },
    {
      "name": "COMPRESS_BTAB_OUTPUT",
      "default": "1"
    },
    {
      "name": "COMPRESS_GAP2_OUTPUT",
      "default": "1"
    }
  ]
}