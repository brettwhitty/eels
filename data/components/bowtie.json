{
  "name": "bowtie",
  "classification": "RNA-Seq",
  "description": "generates bowtie alignment for the input sequence file(s) against the given reference index file.",
  "tool_execution": [
    {
      "name": "bowtie",
      "executable": "{{BIN_DIR}}/bowtie"
    }
  ],
  "converters": {
    "main": [],
    "iterator": []
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "BOWTIE_BIN_DIR",
      "default": "{{BOWTIE_BIN}}"
    },
    {
      "name": "SAMTOOLS_BIN_DIR",
      "default": "{{SAMTOOLS_BIN}}"
    },
    {
      "name": "MODE",
      "default": "v"
    },
    {
      "name": "NUM_MISMATCHES",
      "default": "2"
    },
    {
      "name": "FILE_TYPE",
      "default": "fastq"
    },
    {
      "name": "SEED_LENGTH",
      "default": ""
    },
    {
      "name": "MIN_INSERT",
      "default": ""
    },
    {
      "name": "MAX_INSERT",
      "default": ""
    },
    {
      "name": "LIBRARY_TYPE",
      "default": ""
    },
    {
      "name": "NUM_HITS",
      "default": ""
    },
    {
      "name": "MAX_MULTIHITS",
      "default": ""
    },
    {
      "name": "REPORT_MULTIHITS",
      "default": ""
    },
    {
      "name": "UNMAPPED_READS",
      "default": "unmapped_sequence.txt"
    },
    {
      "name": "NUM_THREADS",
      "default": ""
    },
    {
      "name": "OTHER_PARAMETERS",
      "default": "--sam"
    },
    {
      "name": "OTHER_ARGS",
      "default": "--v"
    }
  ]
}