{
  "name": "quicktree",
  "classification": "",
  "description": "none",
  "tool_execution": [
    {
      "name": "quicktree",
      "executable": "{{QUICKTREE_EXEC}}"
    },
    {
      "name": "dtd validation",
      "executable": "{{BIN_DIR}}/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "quicktree2bsml",
        "name": "quicktree2bsml",
        "input": null,
        "output": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "raw_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "INPUT_TYPE",
      "default": "a"
    },
    {
      "name": "QUICKTREE_OPTS",
      "default": ""
    },
    {
      "name": "COMPRESS_RAW_OUTPUT",
      "default": "1"
    },
    {
      "name": "COMPRESS_BSML_OUTPUT",
      "default": "1"
    }
  ]
}