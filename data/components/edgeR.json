{
  "name": "edgeR",
  "classification": "RNA-Seq",
  "description": "generates edgeR analysis for given sample info and counts filelist",
  "tool_execution": [
    {
      "name": "edgeR",
      "executable": "$;BIN_DIR$;/edgeR"
    }
  ],
  "converters": {
    "main": [],
    "iterator": []
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "ANNOTATION_FILE",
      "default": ""
    },
    {
      "name": "R_BIN_DIR",
      "default": "$;R_BIN$;"
    },
    {
      "name": "R_PARAMS",
      "default": ""
    },
    {
      "name": "R_SCRIPT",
      "default": "$;R_SCRIPT_PATH$;"
    },
    {
      "name": "OTHER_ARGS",
      "default": "--v"
    }
  ]
}