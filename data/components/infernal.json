{
  "name": "infernal",
  "classification": "feature prediction / genes",
  "description": "none",
  "tool_execution": [
    {
      "name": "infernal",
      "executable": "$;BIN_DIR$;/run_cmsearch"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "infernal2bsml",
        "name": "infernal2bsml",
        "input": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw.tbl",
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "raw_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "OTHER_OPTS",
      "default": "--cut_tc"
    },
    {
      "name": "SEQUENCE_LIST",
      "default": ""
    },
    {
      "name": "ANNOT_BSML_LIST",
      "default": ""
    },
    {
      "name": "HMM_CM_TABLE",
      "default": ""
    },
    {
      "name": "CM_FILE",
      "default": "$;CM_FILE_PATH$;"
    },
    {
      "name": "STOCKHOLM_PATH",
      "default": "$;RFAM_STOCKHOLM_DIR$;"
    },
    {
      "name": "DEFAULT_TYPE",
      "default": ""
    }
  ]
}