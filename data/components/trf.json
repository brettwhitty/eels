{
  "name": "trf",
  "classification": "feature prediction / tRNAs",
  "description": "none",
  "tool_execution": [
    {
      "name": "trf",
      "executable": "$;BIN_DIR$;/trf_wrapper"
    },
    {
      "name": "relocate output",
      "executable": "find"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "trf2bsml",
        "name": "trf2bsml",
        "input": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw",
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "raw_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "MATCH",
      "default": "2"
    },
    {
      "name": "MISMATCH",
      "default": "7"
    },
    {
      "name": "DELTA",
      "default": "7"
    },
    {
      "name": "PM",
      "default": "80"
    },
    {
      "name": "PI",
      "default": "10"
    },
    {
      "name": "MINSCORE",
      "default": "50"
    },
    {
      "name": "MAXPERIOD",
      "default": "500"
    },
    {
      "name": "OTHER_OPTS",
      "default": ""
    },
    {
      "name": "COMPRESS_BSML_OUTPUT",
      "default": "0"
    }
  ]
}