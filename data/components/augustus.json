{
  "name": "augustus",
  "classification": "feature prediction / genes",
  "description": "none",
  "tool_execution": [
    {
      "name": "augustus",
      "executable": "$;AUGUSTUS_BINARY_PATH$;"
    },
    {
      "name": "dtd validation",
      "executable": "$;BIN_DIR$;/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "augustus2bsml",
        "name": "augustus2bsml",
        "input": null,
        "output": "$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "raw_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "CDS",
      "default": "on"
    },
    {
      "name": "SPECIES",
      "default": ""
    },
    {
      "name": "SPLIT_SIZE",
      "default": "350000"
    },
    {
      "name": "STRAND",
      "default": "both"
    },
    {
      "name": "GENEMODEL",
      "default": "partial"
    },
    {
      "name": "MAXTRACKS",
      "default": ""
    },
    {
      "name": "EXTRINSIC_CONFIG_FILE",
      "default": "$;AUGUSTUS_CONFIG_PATH$;/extrinsic/extrinsic.cfg"
    },
    {
      "name": "AUGUSTUS_CONFIG_PATH",
      "default": "$;AUGUSTUS_DIR$;/config"
    },
    {
      "name": "AUGUSTUS_BINARY_PATH",
      "default": "$;AUGUSTUS_DIR$;/bin/augustus"
    },
    {
      "name": "OTHER_OPTS",
      "default": ""
    }
  ]
}