{
  "name": "panther",
  "classification": "alignment / model",
  "description": "none",
  "tool_execution": [
    {
      "name": "panther",
      "executable": "{{PANTHER_EXEC}}"
    },
    {
      "name": "dtd validation",
      "executable": "{{BIN_DIR}}/dtdValid.pl"
    }
  ],
  "converters": {
    "main": [],
    "iterator": [
      {
        "script": "panther2bsml",
        "name": "panther2bsml",
        "input": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.raw",
        "output": "{{OUTPUT_DIRECTORY}}/{{ITERATOR_NAME}}/g{{GROUP_NUMBER}}/{{I_FILE_BASE}}.{{COMPONENT_NAME}}.bsml"
      },
      {
        "script": "analysis2bsml",
        "name": "store config params",
        "input": null,
        "output": null
      }
    ]
  },
  "input_modes": {
    "file": false,
    "list": false,
    "directory": false
  },
  "output_formats": [
    "bsml_output_list",
    "raw_output_list"
  ],
  "has_iterator": true,
  "group_count": "150",
  "parameters": [
    {
      "name": "DISPLAY_TYPE",
      "default": "B"
    },
    {
      "name": "HMM_EVALUE_CUTOFF",
      "default": "1e-3"
    },
    {
      "name": "COMPRESS_BSML_OUTPUT",
      "default": "0"
    }
  ]
}